package ct.week02;

import java.util.HashMap;
import java.util.Map;

/*
 * 아이디 비밀번호 찾기
 */
public class Ex26 {
	public static void main(String[] args) {
		Map<String, String> map = new HashMap<>();
		map.put("spring", "1234");
		map.put("summer", "5678");
		map.put("fall", "a1234");
		map.put("winter", "a5678");
	}
}
