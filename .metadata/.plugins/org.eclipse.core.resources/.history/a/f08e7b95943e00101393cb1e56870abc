package ct.week03;

import java.util.LinkedList;
import java.util.Queue;

/*
 * Queue
 */
public class Ex31 {
	public static void main(String[] args) {
		Queue<Integer> que = new LinkedList<>();
		
		que.add(1);
		que.add(2);
		que.offer(3);
		que.offer(4);
		System.out.println(que.toString());
	}
}
