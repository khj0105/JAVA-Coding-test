package ct.week02;

import java.util.ArrayList;
import java.util.List;

/*
 * ArrayList
 * ctrl + shift + o
 */

public class Ex21 {
	public static void main(String[] args) {
		List<String> list = new ArrayList<>();
		list.add("JAVA");
		list.add("Spring");
		list.add("MyBatis");
		System.out.println(list.toString());
		list.add(2, "Oracle");
		
		list.set(3, "Mybatis");
		System.out.println(list.toString());
	}
}
